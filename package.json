{
  "name": "react-gtk",
  "version": "0.0.1",
  "description": "Use React with GTK to write GJS applications",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "types:generate": "rm -rf lib/@types; gi-ts generate",
    "posttypes:generate": "find lib/@types -maxdepth 1 -iname '*.d.ts' -exec sh -c 'f=`echo {} | sed s/.....$//`; mkdir -p $f; mv $f.d.ts $f/index.d.ts' \\;",
    "postinstall": "yarn types:generate",
    "prestart": "webpack",
    "start": "gjs dist/index.js"
  },
  "dependencies": {
    "fast-equals": "^2.0.0",
    "react": "^17.0.1",
    "react-reconciler": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@gi-types/glib": "^2.66.7",
    "@gi-types/gobject": "^2.66.6",
    "@gi-types/gtk": "^3.24.6",
    "@gi.ts/cli": "^1.3.0",
    "@types/react-reconciler": "^0.18.0",
    "babel-loader": "^8.2.2",
    "exports-loader": "^2.0.0",
    "prettier": "^2.2.1",
    "text-loader": "^0.0.1",
    "ts-for-gir": "realh/ts-for-gjs#da2af5d",
    "typescript": "^4.2.0-dev.20210120",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-virtual-modules": "^0.4.2"
  }
}
